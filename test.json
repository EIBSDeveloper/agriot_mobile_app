{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Python Debugger: Django",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver",
                "0.0.0.0:8000",
                "--noreload"
            ],
            "django": true,
            "autoStartBrowser": false,
            "console": "integratedTerminal",
            "jinja": false,
            "program": "${workspaceFolder}\\starterkit\\manage.py"
        }
    ]
}